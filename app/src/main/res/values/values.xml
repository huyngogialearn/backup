    <style name="ActionButton">
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">56dp</item>
        <item name="android:layout_marginEnd">30dp</item>
        <item name="android:layout_marginBottom">40dp</item>
        <item name="android:elevation">0dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="ByPassbase" >
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/passbase_title</item>
        <item name="android:text">@string/by_passbase</item>
        <item name="android:layout_marginTop">18dp</item>
    </style>
    <style name="FullscreenActionBarStyle" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>
    <style name="FullscreenTheme" parent="AppTheme">
        <item name="android:actionBarStyle">@style/FullscreenActionBarStyle</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
        <item name="metaButtonBarStyle">?android:attr/buttonBarStyle</item>
        <item name="metaButtonBarButtonStyle">?android:attr/buttonBarButtonStyle</item>
    </style>
    <style name="ProgressBar" parent="Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/aic_progress_bar</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">6dp</item>
        <item name="android:layout_marginEnd">50dp</item>
    </style>
    <style name="TextAppearance.App.TextInputLayout" parent="@android:style/TextAppearance">
        <item name="android:textColor">#aaaaaa</item>
        <item name="android:textSize">12sp</item>
    </style>
    <style name="TextSubTitle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/passbase_sub_header</item>
        <item name="android:layout_marginTop">8dp</item>
    </style>
    <style name="TextTitle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">30sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/passbase_header</item>
        <item name="android:layout_marginStart">50dp</item>
        <item name="android:layout_marginEnd">50dp</item>
        <item name="android:layout_marginTop">140dp</item>
    </style>
    <declare-styleable name="ActionButton">
        <attr format="boolean" name="loading"/>
    </declare-styleable>
    <declare-styleable name="ButtonBarContainerTheme">
        <attr format="reference" name="metaButtonBarStyle"/>
        <attr format="reference" name="metaButtonBarButtonStyle"/>
    </declare-styleable>
    <declare-styleable name="PassbaseButton">
        <attr format="color" name="backgroundColor"/>
        <attr format="color" name="textColor"/>
    </declare-styleable>
